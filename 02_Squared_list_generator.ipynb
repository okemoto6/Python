   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Point numbers separated by comma (',')1, -1, 456, 21, 9\n",
      "Square of your numbers are: [1, 1, 207936, 441, 81]\n"
     ]
    }
   ],
   "source": [
    "nums = input('Point numbers separated by comma (\\',\\')')\n",
    "num_list = nums.split(',')\n",
    "\n",
    "nums_squared = []\n",
    "\n",
    "for num in num_list:\n",
    "    num = int(num.strip())\n",
    "    nums_squared.append(num ** 2)\n",
    "print(f'Square of your numbers are: {nums_squared}')"
   ]
  }
